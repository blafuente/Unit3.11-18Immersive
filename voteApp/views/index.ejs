    <!DOCTYPE html>
    <html>
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Page Title</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <% include header %>
    </head>
    <body>
        <% include navbar %>
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3 text-center">
                    <h2 class="text-info"><%- msg%></h2>
                    <% if(animals){ %>
                        <h3><%= animals.species %></h3>
                        <img src="/<%= animals.image%>" />
                        <a href="/vote/domestic/<%=animals.id%>">
                            <button class="btn btn-success">Domestic</button>
                        </a>
                        <a href="/vote/wild/<%=animals.id%>">
                            <button class="btn btn-danger">Wild</button>
                        </a>
                    <% } %>
                </div>
            </div>
        </div>    
    </body>
    </html>